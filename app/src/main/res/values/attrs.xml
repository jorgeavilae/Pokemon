<resources>
    <!-- Atributos de la View creada: DPadCountLayout-->
    <declare-styleable name="DPadCountLayout">
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="buttonColor" format="color" />
        <attr name="label" format="string" />
        <attr name="labelTextAppearance" format="reference" />
        <attr name="countTextAppearance" format="reference" />
        <attr name="minCount" format="integer" />
        <attr name="maxCount" format="integer" />
    </declare-styleable>
</resources>